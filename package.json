{
  "name": "readable",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.15.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "scripts": {
    "client": "cd readable && npm start",
    "server": "cd api-server && node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" ",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" "
  }
}
